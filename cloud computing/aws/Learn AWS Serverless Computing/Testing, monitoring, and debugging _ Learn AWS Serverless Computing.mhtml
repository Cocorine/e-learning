appcues-layer .hotspot > iframe.apc-hidden, appcues-layer .appcues-hotspot > iframe.apc-hidden { opacity: 0.01; transform: scale(0.97); visibility: hidden; }

appcues-layer .hotspot > iframe.align-top, appcues-layer .appcues-hotspot > iframe.align-top { transform-origin: center bottom; }

appcues-layer .hotspot > iframe.align-top-left, appcues-layer .appcues-hotspot > iframe.align-top-left { transform-origin: right bottom; }

appcues-layer .hotspot > iframe.align-top-right, appcues-layer .appcues-hotspot > iframe.align-top-right { transform-origin: left bottom; }

appcues-layer .hotspot > iframe.align-bottom, appcues-layer .appcues-hotspot > iframe.align-bottom { transform-origin: center top; }

appcues-layer .hotspot > iframe.align-bottom-left, appcues-layer .appcues-hotspot > iframe.align-bottom-left { transform-origin: right top; }

appcues-layer .hotspot > iframe.align-bottom-right, appcues-layer .appcues-hotspot > iframe.align-bottom-right { transform-origin: left top; }

appcues-debugger { position: fixed; bottom: 0px; left: 15px; overflow-wrap: break-word; z-index: 2147483548; color: white; }

appcues-debugger.expanded { max-height: 450px; min-width: 600px; }

appcues-debugger.collapsed { max-height: 35px; }

appcues-debugger iframe { overflow: hidden; }

appcues-checklist { position: fixed; bottom: 30px; z-index: 2147483647; transition: all 0.5s ease-in-out 0s; }

appcues-checklist.apcl-right { right: 30px; }

appcues-checklist.apcl-right.first-view { right: -300px; }

appcues-checklist.apcl-left { left: 30px; }

appcues-checklist.apcl-left.first-view { left: -300px; }

div.appcues, div.appcues > appcues-layer { position: static; }
