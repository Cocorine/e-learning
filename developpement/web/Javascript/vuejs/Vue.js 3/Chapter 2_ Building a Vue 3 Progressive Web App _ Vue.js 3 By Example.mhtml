#sbo-rt-content hr { box-sizing: content-box; height: 0px; }

#sbo-rt-content h1, #sbo-rt-content h2, #sbo-rt-content h3, #sbo-rt-content h4, #sbo-rt-content .figure-caption { margin-top: 1.2rem; margin-bottom: 1rem; font-weight: normal; }

