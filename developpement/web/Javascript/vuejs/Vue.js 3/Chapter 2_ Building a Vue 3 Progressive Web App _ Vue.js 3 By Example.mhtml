#sbo-rt-content li { font-family: serif; margin-bottom: 0.5rem; margin-top: 0.5rem; }

#sbo-rt-content .callout-heading { font-family: MyriadPro, sans-serif; font-size: 1.2rem; line-height: 1.5; letter-spacing: 0.05rem; text-transform: uppercase; font-weight: lighter; }

