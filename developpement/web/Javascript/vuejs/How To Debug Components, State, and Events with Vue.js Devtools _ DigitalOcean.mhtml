.columns .column.is-1-desktop { flex: 0 0 auto; width: 8.33333%; }

.columns .column.is-offset-1-desktop { margin-left: 8.33333%; }

.columns .column.is-2-desktop { flex: 0 0 auto; width: 16.6667%; }

.columns .column.is-offset-2-desktop { margin-left: 16.6667%; }

.columns .column.is-3-desktop { flex: 0 0 auto; width: 25%; }

.columns .column.is-offset-3-desktop { margin-left: 25%; }

.columns .column.is-4-desktop { flex: 0 0 auto; width: 33.3333%; }

.columns .column.is-offset-4-desktop { margin-left: 33.3333%; }

.columns .column.is-5-desktop { flex: 0 0 auto; width: 41.6667%; }

.columns .column.is-offset-5-desktop { margin-left: 41.6667%; }

.columns .column.is-6-desktop { flex: 0 0 auto; width: 50%; }

.columns .column.is-offset-6-desktop { margin-left: 50%; }

.columns .column.is-7-desktop { flex: 0 0 auto; width: 58.3333%; }

.columns .column.is-offset-7-desktop { margin-left: 58.3333%; }

.columns .column.is-8-desktop { flex: 0 0 auto; width: 66.6667%; }

.columns .column.is-offset-8-desktop { margin-left: 66.6667%; }

.columns .column.is-9-desktop { flex: 0 0 auto; width: 75%; }

.columns .column.is-offset-9-desktop { margin-left: 75%; }

.columns .column.is-10-desktop { flex: 0 0 auto; width: 83.3333%; }

.columns .column.is-offset-10-desktop { margin-left: 83.3333%; }

.columns .column.is-11-desktop { flex: 0 0 auto; width: 91.6667%; }

.columns .column.is-offset-11-desktop { margin-left: 91.6667%; }

.columns .column.is-12-desktop { flex: 0 0 auto; width: 100%; }

.columns .column.is-offset-12-desktop { margin-left: 100%; }

@media only screen and (max-width: 1024px) {
  .column.is-4, .column.is-7 { width: 100% !important; }
}

.slices { position: relative; }

.box { background-color: rgb(255, 255, 255); border-radius: 6px; box-shadow: rgba(10, 10, 10, 0.1) 0px 2px 3px, rgba(10, 10, 10, 0.1) 0px 0px 0px 1px; color: rgb(77, 79, 3); display: block; padding: 1.25rem; }

.buttons { align-items: center; display: flex; flex-wrap: wrap; justify-content: flex-start; }

.buttons:last-child { margin-bottom: -0.5rem; }

@media print, screen and (min-width: 769px) {
  .columns:not(.is-desktop), .tile:not(.is-child) { display: flex; }
  .column.is-7, .column.is-7-tablet { flex: 0 0 auto; width: 58.3333%; }
}
