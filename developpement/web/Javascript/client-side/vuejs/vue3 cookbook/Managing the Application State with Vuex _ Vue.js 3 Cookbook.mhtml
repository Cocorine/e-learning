#sbo-rt-content .packt_italics { font-size: 11pt; }

#sbo-rt-content a, #sbo-rt-content .packt_url { font-size: 12.2pt; }

#sbo-rt-content p, #sbo-rt-content .packt_para { font-size: 11pt; line-height: 1.5em; }

#sbo-rt-content .CDPImageAlign { max-width: 100%; max-height: 100%; }

#sbo-rt-content div.CDPAlign, #sbo-rt-content p.CDPAlign { margin-top: 0.01em; margin-bottom: 0.01em; }

#sbo-rt-content li { font-size: 11pt; line-height: 1.5em; }

#sbo-rt-content div.CDPAlignCentre, #sbo-rt-content div.CDPAlign { margin-top: 1rem; margin-left: auto; margin-right: auto; text-align: center; width: 100%; max-width: 100%; break-before: auto; break-inside: avoid !important; }

#sbo-rt-content img { max-width: 100%; }
