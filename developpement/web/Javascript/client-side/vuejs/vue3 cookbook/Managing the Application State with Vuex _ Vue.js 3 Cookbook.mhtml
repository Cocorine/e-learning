#sbo-rt-content h3, #sbo-rt-content .packt_head3 { font-size: 1.17em; }

#sbo-rt-content h4, #sbo-rt-content .packt_head4 { font-size: 1em; }

#sbo-rt-content code, #sbo-rt-content .packt_code, #sbo-rt-content .packt_codeblock, #sbo-rt-content pre { font-size: 11pt; margin-left: 1.25em; padding-left: 0.3em; margin-bottom: 0.01em; }

#sbo-rt-content .packt_figref { font-size: 11pt; }

#sbo-rt-content q, #sbo-rt-content .packt_quote { font-size: 11pt; }

