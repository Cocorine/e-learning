<ul>
<li><kbd><span>withMethod($method)</span></kbd></li>
<li><kbd>withRequestTarget($requestTarget)</kbd></li>
<li><kbd>withUri(UriInterface $uri, $preserveHost = false)</kbd></li>
<li><kbd>withCookieParams(array $cookies)</kbd></li>
<li><kbd>withQueryParams(array $query)</kbd></li>
<li><kbd>withUploadedFiles(array $uploadedFiles)</kbd></li>
<li><kbd>withParsedBody($data)</kbd></li>
