#book-content #sbo-rt-content code { font-family: Consolas, Courier, monospace; }

#book-content #sbo-rt-content caption, #book-content #sbo-rt-content th { text-align: left; }

#book-content #sbo-rt-content h1, #book-content #sbo-rt-content h2, #book-content #sbo-rt-content h3, #book-content #sbo-rt-content h4, #book-content #sbo-rt-content h5, #book-content #sbo-rt-content h6 { font-size: 100%; font-weight: normal; }

